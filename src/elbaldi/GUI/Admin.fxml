<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="elbaldi.GUI.AdminController">
   <children>
      <VBox layoutY="137.0" prefHeight="346.0" prefWidth="171.0" style="-fx-background-color: #ffffff;">
         <children>
            <ComboBox fx:id="Promotionfix" onAction="#select" prefHeight="25.0" prefWidth="175.0" promptText="Promotion" style="-fx-background-color: #f00520;" />
            <ComboBox fx:id="Questionfix" onAction="#choisir" prefHeight="25.0" prefWidth="173.0" promptText="Question" style="-fx-background-color: #f00520;" />
            <ComboBox fx:id="quizfix" onAction="#selectquiz" prefHeight="25.0" prefWidth="182.0" promptText="Quiz" style="-fx-background-color: #f00520;" />
         </children>
      </VBox>
      <Label layoutX="470.0" layoutY="14.0" text="Admin" textFill="#f20c0c">
         <font>
            <Font name="Arial Black" size="31.0" />
         </font>
      </Label>
      <ImageView fitHeight="66.0" fitWidth="72.0" pickOnBounds="true" smooth="false">
         <image>
            <Image url="@../../Ressource/elbaldi.jpg" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
