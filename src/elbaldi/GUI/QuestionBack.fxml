<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane id="AnchorPane" onMouseMoved="#afficheronclick" prefHeight="627.0" prefWidth="794.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="elbaldi.GUI.QuestionBackController">
   <children>
      <HBox layoutY="18.0" prefHeight="632.0" prefWidth="869.0">
         <children>
            <ScrollPane fx:id="scroll" prefHeight="614.0" prefWidth="869.0">
               <HBox.margin>
                  <Insets top="40.0" />
               </HBox.margin>
               <content>
                  <GridPane fx:id="gridd" prefHeight="159.0" prefWidth="870.0">
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                  </GridPane>
               </content>
            </ScrollPane>
         </children>
      </HBox>
      <Button fx:id="back" layoutY="2.0" mnemonicParsing="false" onAction="#goBack" prefHeight="25.0" prefWidth="56.0" styleClass="quiz-item" stylesheets="@quiz.css" text="Back" />
      <ImageView fitHeight="31.0" fitWidth="29.0" layoutX="684.0" layoutY="4.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Ressource/ajouter.png" />
         </image>
      </ImageView>
      <TextField fx:id="quizid" layoutX="754.0" layoutY="4.0" prefHeight="14.0" prefWidth="14.0" />
      <Button fx:id="ajouterfx" layoutX="714.0" layoutY="4.0" mnemonicParsing="false" onAction="#ajouter" styleClass="quiz-item" stylesheets="@quiz.css" text="Ajouter Question" />
   </children>
</AnchorPane>
